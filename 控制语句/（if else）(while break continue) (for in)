#⑧控制语句
Num_e = 4
Num_f = 12
if Num_f > Num_e:
    print("Num_f > Num_e")
elif Num_f == Num_e:
    print("Num_f == Num_e")
else:
    print("else Num_f < Num_e")

#⑨循环语句
while Num_e < Num_f:
    print(Num_e)
    if Num_e < 9:
        Num_e += 2
        continue     #忽略后面的语句，立即进入下一个循环
    Num_e += 1
    print(f"循环语句2：{Num_e}") # 注意学习这种字符串与变量的混合输出
    if Num_e == 8:
        break         #跳出循环
